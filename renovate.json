{
  "customManagers": [
    {
      "customType": "regex",
      "fileMatch": ["\\.github\\/.*\\.ya?ml$"],
      "matchStrings": [
        "uses:\\s(?<packageName>pankajmouriyakong\\/shared-actions-pankaj)\\/.*@(?<depName>[^_]+)_(?<currentValue>[0-9\\.]+)"
      ],
      "datasourceTemplate": "github-tags",
      "extractVersionTemplate": "{{currentValue}}"
    }
  ]
}
