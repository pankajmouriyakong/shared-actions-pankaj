{
    "extends": ["config:base"],
    "packageRules": [
      {
        "matchManagers": ["github-actions"],
        "matchPaths": [
          ".github/workflows/*.yml",
          ".github/workflows/*.yaml"
        ],
        "matchPackageNames": [
          "@code-build-actions/*",
          "@code-check-actions/*",
          "@pr-previews/*",
          "@security-actions/*",
          "@slack-actions/*"
        ],
        "sourceUrl": "https://github.com/pankajmouriyakong/shared-actions-pankaj",
        "enabled": true
      }
    ]
  }