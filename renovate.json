{
    "extends": [
      "github>pankajmouriyakong/public-shared-renovate:github-actions"
    ],
    "customManagers": [
      {
        "customType": "regex",
        "fileMatch": ["^/security-actions/semgrep/run-semgrep/action\\.yml$"],
        "matchStrings": [
          "semgrep/semgrep:(?<currentValue>[\\d\\.]+)"
        ],
        "datasourceTemplate": "docker",
        "depNameTemplate": "semgrep/semgrep",
        "extractVersionTemplate": "^(?:v)?(?<version>[0-9\\.]+)$"
      }
    ],
    "hostRules": [
      {
        "hostType": "docker",
        "domainName": "docker.io",
        "enabled": true
      }
    ],
    "packageRules": [
      {
        "matchDatasources": ["docker"],
        "matchPackageNames": ["semgrep/semgrep"],
        "registryUrls": ["https://index.docker.io/v2/"],
        "versioning": "semver"
      }
    ],
    "ignorePaths": [
      "**/k-internal/**"
    ]
  }
  